  }, {
    "type" : "javascript-index",
    "id" : "bomeu3di",
    "script" : "function(doc) {\n  if (doc.hasField(\"category\")) {\n    if (doc.getFirstFieldValue(\"category\").equals(\"blog\")) {\n      doc.setField(\"type\", \"post\");\n    } else {\n      doc.setField(\"type\", \"page\");\n    }\n  } else {\n    doc.setField(\"type\", \"page\")\n  }\n  \n  return doc;\n}",
