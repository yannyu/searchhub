{
  "name": "turbine",
    "description": "The Apache Turbine project, tweets, wiki, github, et. al",
  "label": "Apache Turbine",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "turbine-website",
      "label": "Apache Turbine Website",
      "url": "https://turbine.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "turbine-user",
      "label": "Turbine User",
      "mbox": "turbine-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "turbine-dev",
      "label": "Turbine Dev",
      "mbox": "turbine-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-turbine",
    "label": "Apache turbine JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/TRB",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
